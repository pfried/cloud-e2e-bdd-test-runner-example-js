{
  "name": "@nordicsemiconductor/e2e-bdd-test-runner-example",
  "version": "0.0.0-development",
  "description": "Example use of @nordicsemiconductor/e2e-bdd-test-runner",
  "main": "index.js",
  "scripts": {
    "test": "node dist/feature-runner/run-features.js",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-example-js.git"
  },
  "keywords": [
    "bdd",
    "aws",
    "cloudnative",
    "gherkin",
    "testing"
  ],
  "author": "Markus Tacker <m@coderbyheart.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-example-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-example-js#readme",
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.141.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.104",
    "@nordicsemiconductor/cloudformation-helpers": "7.0.6",
    "@nordicsemiconductor/e2e-bdd-test-runner": "15.1.62",
    "@types/node": "17.0.31",
    "aws-cdk-lib": "2.33.0",
    "cdk": "2.33.0",
    "constructs": "10.1.63"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "dependencies": {
    "aws-sdk": "2.1166.0"
  }
}
